<view hidden="{{!(visible)}}" class="w-calendar"><view class="w-calendar-header"><view data-event-opts="{{[['tap',[['prev',['$event']]]]]}}" class="prev" bindtap="__e"></view><view class="current">{{year+"年"+month+"月"}}</view><view data-event-opts="{{[['tap',[['next',['$event']]]]]}}" class="next" bindtap="__e"></view><view data-event-opts="{{[['tap',[['back',['$event']]]]]}}" class="back" bindtap="__e">返回今日</view></view><swiper duration="{{400}}" circular="{{true}}" current="{{swiperIndex}}" data-event-opts="{{[['change',[['swiperChange',['$event']]]]]}}" bindchange="__e"><block wx:for="{{dataList}}" wx:for-item="month" wx:for-index="mIndex" wx:key="mIndex"><swiper-item><view class="w-calendar-item"><view class="w-calendar-week"><block wx:for="{{weeks}}" wx:for-item="week" wx:for-index="wIndex" wx:key="wIndex"><view class="w-week-item">{{week}}</view></block></view><view class="w-calendar-day"><block wx:for="{{month.data}}" wx:for-item="item" wx:for-index="dIndex" wx:key="dIndex"><view data-event-opts="{{[['tap',[['checkItem',['$0'],[[['dataList','',mIndex],['data','',dIndex]]]]]]]}}" class="{{['w-day-item',[(item.type=='before'||item.type=='after')?'disabled':''],[(item.active)?'active':''],[(disabledBefore&&item.disabledBefore)?'disabled-before':''],[(disabledAfter&&item.disabledAfter)?'disabled-after':'']]}}" bindtap="__e"><view class="box"><text>{{item.day}}</text><block wx:if="{{lunar}}"><text>{{item.lunar.IDayCn}}</text></block></view></view></block></view></view></swiper-item></block></swiper></view>